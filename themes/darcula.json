{
  "name": "Darcula Storm",
  "type": "dark",
  "colors": {
    "editor.background": "#232525",
    "editor.foreground": "#a9b7c6",
    "editor.selectionBackground": "#214283",
    "editor.lineHighlightBackground": "#323232",
    "editorGutter.background": "#313335",
    "titleBar.activeBackground": "#ffffff1a",
    "titleBar.inactiveBackground": "#ffffff10",
    "titleBar.activeForeground": "#a9b7c6",
    "sideBar.border": "#333436",
    "sideBar.foreground": "#b6b4b3",
    "sideBar.background": "#3c3f41",
    "sideBar.inactiveBackground": "#3c3f41",
    "sideBarSectionHeader.background": "#3b4754",
    "sideBarSectionHeader.border": "#333436",
    "statusBar.foreground": "#a9b7c6",
    "statusBar.background": "#313335",
    "statusBar.border": "#333436",
    "tab.activeBackground": "#ffffff1a",
    "tab.inactiveBackground": "#ffffff0b",
    "tab.activeBorder": "#4a88c7",
    "editorSuggestWidget.background": "#303030",
    "editorSuggestWidget.highlightForeground": "#cc6e2f",
    "editorSuggestWidget.selectedBackground": "#204182cc",
    "editorLineNumber.activeForeground": "#a4a3a3",
    "editorLineNumber.foreground": "#606366",
    "badge.background": "#ffffff1a",
    "activityBar.background": "#ffffff1a",
    "activityBarBadge.background": "#4a88c7"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "name": "Operator Keywords",
      "scope": [
        "keyword.operator",
        "keyword.operator.logical",
        "keyword.operator.relational",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.ternary",
        "keyword.operator.arithmetic",
        "keyword.operator.spread"
      ],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Strings",
      "scope": "string,string.character.escape,string.template.quoted,string.template.quoted.punctuation,string.template.quoted.punctuation.single,string.template.quoted.punctuation.double,string.type.declaration.annotation,string.template.quoted.punctuation.tag",
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "String Interpolation Begin and End",
      "scope": "punctuation.definition.template-expression.begin,punctuation.definition.template-expression.end",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Escaped sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#CC7832",
        "fontStyle": ""
      }
    },
    {
      "name": "String Interpolation Body",
      "scope": [
        "expression.string",
        "meta.template.expression",
        "string.quoted.double.php punctuation.definition.variable.php",
        "string.unquoted.heredoc.php punctuation.definition.string.php",
        "string.unquoted.heredoc.php punctuation.definition.variable.php",
        "string.quoted.double.php punctuation.definition.arguments.begin.bracket.round.php",
        "string.quoted.double.php punctuation.definition.arguments.end.bracket.round.php",
        "string.unquoted.heredoc.php punctuation.definition.arguments.begin.bracket.round.php",
        "string.unquoted.heredoc.php punctuation.definition.arguments.end.bracket.round.php",
        "string.quoted.double.php punctuation.section.array.begin.bracket.round.php",
        "string.quoted.double.php punctuation.section.array.end.bracket.round.php",
        "string.unquoted.heredoc.php punctuation.section.array.begin.php",
        "string.unquoted.heredoc.php punctuation.section.array.end.php"
      ],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Number && html entities",
      "scope": [
        "constant.numeric",
        "constant.character.entity",
        "constant.character.entity entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#7A9EC2",
        "fontStyle": ""
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language", "variable.language"],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#9E7BB0",
        "fontStyle": " italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.new",
        "keyword.operator.delete",
        "keyword.operator.static",
        "keyword.operator.this",
        "keyword.operator.type",
        "keyword.operator.expression"
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Types, Class Types",
      "scope": "entity.name.type,meta.return.type,meta.type.annotation,meta.type.parameters,support.type.primitive",
      "settings": {
        "foreground": "#7A9EC2"
      }
    },
    {
      "name": "Expression terminator and param delimiters",
      "scope": [
        "punctuation.terminator.statement",
        "punctuation.terminator.expression",
        "punctuation.separator.delimiter",
        "meta.delimiter.comma"
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Storage type",
      "scope": ["storage", "storage.type", "storage.modifier", "storage.arrow"],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "JS Arrow function's arrow",
      "scope": "storage.type.function.arrow.js",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Function names",
      "scope": [
        "class.instance.constructor",
        "support.function.constructor",
        "new.expr entity.name.type",
        "support.function",
        "support.function.magic",
        "entity.name.function",
        "meta.method-call.php entity.name.function.php",
        "meta.class.body.php meta.function.php entity.name.function.php"
      ],
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "PHP Function names (excluding class methods)",
      "scope": [
        "source.php support.function",
        "meta.function.php entity.name.function.php",
        "meta.function-call.php entity.name.function.php"
      ],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Function Types",
      "scope": "annotation.meta.ts, annotation.meta.tsx",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Function Argument",
      "scope": "variable.parameter, operator.rest.parameters",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Variable, Property",
      "scope": [
        "variable.property",
        "variable.other.property",
        "variable.other.object.property",
        "variable.object.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#9E7BB0"
      }
    },
    {
      "name": "Module Name",
      "scope": "quote.module",
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "entity.name.tag",
        "punctuation.definition.tag.jsx"
      ],
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#bababa"
      }
    },
    {
      "name": "Tag attribute = symbol",
      "scope": [
        "JSXAttrs keyword.operator.assignment",
        "text.html.twig meta.tag.block.any.html",
        "text.html.twig meta.tag.inline.any.html"
      ],
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "Object Keys",
      "scope": [
        "meta.object-literal.key",
        "constant.other.object.key.js string.unquoted.js"
      ],
      "settings": {
        "foreground": "#9E7BB0",
        "fontStyle": ""
      }
    },
    {
      "name": "TypeScript Class Modifiers",
      "scope": "storage.modifier.ts",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "TypeScript Type Casting",
      "scope": "ts.cast.expr,ts.meta.entity.class.method.new.expr.cast,ts.meta.entity.type.name.new.expr.cast,ts.meta.entity.type.name.var-single-variable.annotation,tsx.cast.expr,tsx.meta.entity.class.method.new.expr.cast,tsx.meta.entity.type.name.new.expr.cast,tsx.meta.entity.type.name.var-single-variable.annotation",
      "settings": {
        "foreground": "#7A9EC2"
      }
    },
    {
      "name": "TypeScript Type Declaration",
      "scope": "ts.meta.type.support,ts.meta.type.entity.name,ts.meta.class.inherited-class,tsx.meta.type.support,tsx.meta.type.entity.name,tsx.meta.class.inherited-class,type-declaration,enum-declaration",
      "settings": {
        "foreground": "#7A9EC2"
      }
    },
    {
      "name": "TypeScript Method Declaration",
      "scope": "function-declaration,method-declaration,method-overload-declaration,type-fn-type-parameters",
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "Documentation Block",
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#629755"
      }
    },
    {
      "name": "Documentation Highlight (JSDoc)",
      "scope": "storage.type.class.jsdoc",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Documentation Tag (PHPDoc)",
      "scope": "keyword.other.phpdoc.php",
      "settings": {
        "foreground": "#629755",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Documentation Type (PHPDoc)",
      "scope": ["meta.other.type.phpdoc.php", "keyword.other.array.phpdoc.php"],
      "settings": {
        "foreground": "#a9b7c6",
        "fontStyle": ""
      }
    },
    {
      "name": "Import-Export-All (*) Keyword",
      "scope": "constant.language.import-export-all",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Object Key Seperator",
      "scope": "objectliteral.key.separator, punctuation.separator.key-value",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Regex",
      "scope": "regex",
      "settings": {
        "fontStyle": " italic"
      }
    },
    {
      "name": "Typescript Namespace",
      "scope": "ts.meta.entity.name.namespace,tsx.meta.entity.name.namespace",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Regex Character-class",
      "scope": "regex.character-class",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Class Name && inheritance",
      "scope": [
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.name.type.trait.php",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Documentation Entity",
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "YAML entity",
      "scope": "yaml.entity.name,yaml.string.entity.name",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "YAML string value",
      "scope": "yaml.string.out",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Ignored (Exceptions Rules)",
      "scope": "meta.brace.square.ts,block.support.module,block.support.type.module,block.support.function.variable,punctuation.definition.typeparameters.begin,punctuation.definition.typeparameters.end",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Regex Group/Set",
      "scope": "punctuation.definition.group.regexp,punctuation.definition.character-class.regexp",
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "Regex Character Class",
      "scope": "constant.other.character-class.regexp, constant.character.escape.ts",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Regex Or Operator",
      "scope": "expr.regex.or.operator",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Tag string",
      "scope": "string.template.tag,string.template.punctuation.tag,string.quoted.punctuation.tag,string.quoted.embedded.tag, string.quoted.double.tag",
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "Tag function parenthesis",
      "scope": "tag.punctuation.begin.arrow.parameters.embedded,tag.punctuation.end.arrow.parameters.embedded",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Object-literal key class",
      "scope": "object-literal.object.member.key.field.other,object-literal.object.member.key.accessor,object-literal.object.member.key.array.brace.square",
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "CSS Property-value",
      "scope": "property-list.property-value,property-list.constant",
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "CSS Property variable",
      "scope": "support.type.property-name.variable.css,support.type.property-name.variable.scss,variable.scss",
      "settings": {
        "foreground": "#7A9EC2"
      }
    },
    {
      "name": "CSS Property entity",
      "scope": "entity.other.attribute-name.class.css,entity.other.attribute-name.class.scss,entity.other.attribute-name.parent-selector-suffix.css,entity.other.attribute-name.parent-selector-suffix.scss",
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "CSS Property-value",
      "scope": "property-list.property-value.rgb-value, keyword.other.unit.css,keyword.other.unit.scss",
      "settings": {
        "foreground": "#7A9EC2"
      }
    },
    {
      "name": "CSS Property-value function",
      "scope": "property-list.property-value.function",
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "CSS constant variables",
      "scope": "support.constant.property-value.css,support.constant.property-value.scss",
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "CSS Tag",
      "scope": "css.entity.name.tag,scss.entity.name.tag",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "CSS ID, Selector",
      "scope": "meta.selector.css, entity.attribute-name.id, entity.other.attribute-name.pseudo-class.css,entity.other.attribute-name.pseudo-element.css",
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "CSS Keyword",
      "scope": "keyword.scss,keyword.css",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Triple-slash Directive Tag",
      "scope": "triple-slash.tag",
      "settings": {
        "foreground": "#a9b7c6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },
    {
      "name": "Python operators",
      "scope": "keyword.operator.logical.python",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Dart class type",
      "scope": "support.class.dart",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "PHP variables",
      "scope": [
        "variable.language.php",
        "variable.other.php",
        "variable.other.property.php",
        "variable.language.this.php",
        "string.quoted.double.php variable.other.php punctuation.definition.variable.php",
        "string.unquoted.heredoc.php variable.other.php punctuation.definition.variable.php"
      ],
      "settings": {
        "foreground": "#9E7BB0"
      }
    },
    {
      "name": "Perl specific",
      "scope": ["variable.other.readwrite.perl"],
      "settings": {
        "foreground": "#9E7BB0"
      }
    },
    {
      "name": "PHP variables",
      "scope": ["support.variable.property.php"],
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "PHP namespace",
      "scope": ["entity.name.type.namespace.php"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "PHP open/close tags",
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Attributes",
      "scope": ["meta.attribute.php"],
      "settings": {
        "foreground": "#bbb529"
      }
    },
    {
      "name": "Attributes > parenthesis",
      "scope": [
        "meta.attribute.php punctuation.definition.arguments.begin.bracket.round.php",
        "meta.attribute.php punctuation.definition.arguments.end.bracket.round.php",
        "meta.attribute.php punctuation.section.array.begin.php",
        "meta.attribute.php punctuation.section.array.end.php",
        "meta.attribute.php support.class.php"
      ],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Attributes > parameter separator",
      "scope": ["meta.attribute.php punctuation.separator.delimiter.php"],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "Named arguments",
      "scope": [
        "entity.name.variable.parameter.php",
        "meta.function-call.php punctuation.separator.colon.php",
        "meta.attribute.php punctuation.separator.colon.php"
      ],
      "settings": {
        "foreground": "#467cda"
      }
    },
    {
      "name": "Twig",
      "scope": [
        "punctuation.separator.property.twig",
        "punctuation.section.tag.twig",
        "variable.other.twig",
        "variable.other.property.twig",
        "support.function.twig",
        "meta.tag.template.value.twig"
      ],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Twig operators",
      "scope": [
        "keyword.operator.logical.twig",
        "keyword.operator.comparison.twig"
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    }
  ]
}
